<Window x:Class="Mute.ExceptionConfigurationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mute"
        mc:Ignorable="d"
        Title="ExceptionConfigurationWindow" Height="450" Width="800">
    <Grid>
        <Label Content="Check any program you want to exclude from auto-mute from the list below" />
        <ScrollViewer x:Name="ScrollViewer" Width="750" Height="370" Margin="0,0,0,0"/>

    </Grid>
</Window>
